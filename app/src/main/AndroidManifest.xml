<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chandra.elemento">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_launch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_launch_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Elemento"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ChemistryActivity"
            android:exported="false" />
        <activity
            android:name=".PhysicsActivity"
            android:exported="false" />
        <activity
            android:name=".MathsActivity"
            android:exported="false" />
        <activity
            android:name=".CategoryActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>